   "source": [
    "for e in range(n_episodes):\n",
    "    state=env.reset()\n",
    "    state = np.reshape(state,[1,state_size])\n",
    "    batch_size=32\n",
    "    \n",
    "    for t in range(500):\n",
    "        env.render()\n",
    "        action=agent.act(state) # 0 or 1\n",
    "        next_state,reward,done,other_info=env.step(action)\n",
    "        reward = reward if not done else -10\n",
    "        next_state=np.reshape(next_state,[1,state_size])\n",
    "        agent.remember(state,action,reward,next_state,done) #Experiece for the agent\n",
    "        \n",
    "        if done:\n",
    "            print(\"Game episode :{}/{} High score :{} Exploration rate:{:.2}\".format(e,20,t,agent.epsilon))\n",
    "            break\n",
    "    if len(agent.memory)> batch_size:\n",
    "        agent.train(batch_size)\n",
    "    \n",
    "    if e%50==0:\n",
    "        agent.save(output_dir+\"weights_\"+'{:04d}'.format(e)+\".hdf5\")\n",
    "        \n",
    "print(\"Deep Q learning Model trained\")\n",
    "env.close()"
   ]
